{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./.ts/",
    "inlineSourceMap": true,
    "sourceMap": false,
    "declaration": true,
    "module": "commonjs",
    "strictPropertyInitialization": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es6",
    "lib": [
      "es2017",
      "dom"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "jasmine",
      "jasmine-expect",
      "node"
    ]
  }
}
